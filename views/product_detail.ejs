    <script>
        //Set the countdown timer duration
        let countdownDuration = 5400000; // 1 hour 30 minutes in milliseconds

        // Update the time every second
        let timer = setInterval(function () {
            // Calculate the time remaining
            countdownDuration -= 1000;

             // Check if the time has expired
            if (countdownDuration < 0) {
                clearInterval(timer);
                countdownDuration = 0;
            }

            // Format the time remaining as hours, minutes, and seconds
            let hours = Math.floor((countdownDuration / (1000 * 60 * 60)) % 24);
            let minutes = Math.floor((countdownDuration / (1000 * 60)) % 60);
            let seconds = Math.floor((countdownDuration / 1000) % 60);

            // Display the time remaining in the HTML element with ID "timer"
            document.getElementById("timer").innerHTML = hours + "  :  " + minutes + "  :  " + seconds;
        }, 1000); // Update every second





    </script>
